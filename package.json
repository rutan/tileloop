{
  "name": "tileloop",
  "version": "1.0.0",
  "main": "index.js",
  "author": "ru_shalm <ru_shalm@hazimu.com>",
  "private": true,
  "scripts": {
    "start": "parcel src/index.html",
    "build": "run-s clean build:*",
